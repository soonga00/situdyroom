{
  "expo": {
    "name": "SitudyRoom",
    "slug": "SitudyRoom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/situdyroomIconS.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/situdySplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./situdyroomIconR.png"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}